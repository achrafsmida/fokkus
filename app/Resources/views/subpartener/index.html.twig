{% extends 'base.html.twig' %}

{% block body %}
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Sub Partener |
                <small>List</small>
                <a class="btn btn-success btn-xs pull-right" href="{{ path('subpartener_new') }}">Create a new group</a>

            </div>
            <div class="panel-body">

                <table id="datatable2" class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Lastname</th>
                <th>Firstname</th>
                <th>Mail</th>
                <th>Twitter</th>
                <th>Society</th>
                <th>Updateddate</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for subPartener in subParteners %}
            <tr>
                <td><a href="{{ path('subpartener_show', { 'id': subPartener.id }) }}">{{ subPartener.id }}</a></td>
                <td>{{ subPartener.lastName }}</td>
                <td>{{ subPartener.firstName }}</td>
                <td>{{ subPartener.mail }}</td>
                <td>{{ subPartener.twitter }}</td>
                <td>{{ subPartener.society }}</td>
                <td>{% if subPartener.updatedDate %}{{ subPartener.updatedDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                  
                            <a  class="btn btn-warning" href="{{ path('subpartener_show', { 'id': subPartener.id }) }}">show</a>
                        
                            <a class="btn btn-danger" href="{{ path('subpartener_edit', { 'id': subPartener.id }) }}">edit</a>
                       
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
